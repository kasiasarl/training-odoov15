<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="course_wizard_form" model="ir.ui.view">
            <field name="name">Course Wizard</field>
            <field name="model">course.wizard</field>
            <field name="arch" type="xml">
                <form string="">
                    <group>
                        <field name="name"/>
                        <field name="new_date"/>
                    </group>
                    <footer>
                        <button name="run_validate" string="Validate" type="object" class="oe_highlight"/>
                        <button string="Cancel" class="oe_link" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

    </data>
</odoo>